<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="219dp"
    android:layout_height="55dp"
    android:background="@mipmap/weizhiduodingbiaotilan">

    <TextView
        android:id="@+id/tv_dangqian_cheliang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="12dp"
        android:includeFontPadding="false"
        android:text="当前车辆位置"
        android:textColor="#ff111111"
        android:textSize="17sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="23dp"

        android:includeFontPadding="false"
        android:text="到这里"
        android:textColor="#ffffffff"
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_dangqian_cheliang"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tv_dangqian_cheliang" />
</androidx.constraintlayout.widget.ConstraintLayout>